#include<stdio.h>
void main()
{
    int a,b,c;
    printf("enter a number:");
    scanf("%d",&a);
    printf("enter a number:");
    scanf("%d",&b);
    printf("enter a number:");
    scanf("%d",&c);
    printf("\nanswer=%d\nanswer=%d\nanswer=%d",a,b,c);
    // (output: answer=2,answer=3,answer=4)isme jo hum likhenge vhi print hoga

    printf("\nanswer=%d+%d=%d\nanswer=%d+%d=%d\nanswer=%d+%d+%d=%d",a,b,a+b,b,c,b+c,c,a,b,c+a+b);
    // (output:answer:2+3=5,answer=3+4=7,answer=4+2+3=9)isme kis kis ke sath plus ho rha hai vo dikhi dega

    printf("\nanswer=%d\nanswer=%d\nanswer=%d",a+b,b+c,c+a+b);
    //  (output:answer=5,answer=7,answer=9)isme sidha answer dikhta hai
}
