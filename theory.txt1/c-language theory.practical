(5)increment/decrement:


        #include<stdio.h>
void main()
{
    //ye plus hai
    int plus=5;
    printf("ANS=%d",plus++);
    //(plus++) agr ye pehle hai to isme jo value hai vhi btaye ga 5 hi btayega iske bad jo bhi hoga usme 1 puse hoga
    printf("\nANS=%d",plus);
    printf("\nANS=%d",++plus);//(++plus) isme pehle se hi 1 plus huva rhega
    
       /*output: ANS=5
                ANS=6
                ANS=7
        */

    //ye minus hai
    int minus=10;
    printf("ANS=%d",minus--);
    //(minus--)agr ye pehle hai to isme jo value hai vhi btaye ga 10 hi btayega iske bad jo bhi hoga usme 1 minus hoga
    printf("\nANS=%d",minus);
    printf("\nANS=%d",--minus);//(--minus) isme pehle se hi 1 minus huva rhega
    
    /*output: ANS=10
              ANS=9
              ANS=8
      */

    /*agr hum pehe print me plus++ karege or duseme ++plus karenge to ouput 3 ayrga 
      e.g.    int plus=1
       printf("ANS=%d",plus++);
       printf("ANS=%d",++plus);
       
        output: ANS=3
        esa hi minus me bhi hoga uska output -1 ayega*/ 
