//declaration,initlization,use.
// adition(+),subtraction(-),multiplication(*),division(/)
#include <stdio.h>
void main()
{
    int num;
    num =10;
    printf("adition:%d+%d+%d=%d",num,num,num,num+num+num); // ye he adition
    printf("\nsubtraction:%d-%d=%d",num,num,num-num); //ye subtraction
    printf("\nmultiplication:%d*%d=%d",num,num,num*num); //ye multiplication
    printf("\ndivision:%d/%d=%d",num,num,num/num);  // ye hai division
}


output: adition:10+10+10=30
        subtraction:10-10=0
        multiplication:10*10=100
        division:10/10=1
