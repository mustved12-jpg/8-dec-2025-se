#include<iostream>
using namespace std;
class simple
{
	public:
		int a;
		string name;
};
main()
{
	simple obj=simple();
	
	obj.a=3;
	obj.name="mustved";
	
	cout<<"a="<<obj.a<<"\nname="<<obj.name<<endl;
	
}
/*
output:
a=3
name=mustved
/