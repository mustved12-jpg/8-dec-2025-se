PRO.3                                                                          <<16-02-26>>
#include<iostream>
using namespace std;
class Student
{
	public:
		int id;
		string name;
		
		Student()
		{
			cout<<":::HELLO:::";
			id=10;
			name="mustved";
		}
		void dis()
		{
			cout<<"\nid="<<id<<"\nname="<<name;
		}
};
main()
{
	Student s1;//agar humko ye dubara print karana ho to Student s2 ya s1 ke alava kusch or likhana hoga.
	Student s2;
	s1.dis();
	s2.dis();
}
/*output:
:::HELLO::::::HELLO:::
id=10
name=mustved
id=10
name=mustved
*/
----------------------------------
#include<iostream>
using namespace std;
class Student
{
	public:
		int id;
		string name;
		
		Student()
		{
			cout<<"\n:::HELLO:::";
			id=10;
			name="mustved";
		}
		void dis()
		{
			cout<<"\nid="<<id<<"\nname="<<name;
		}
};
main()
{
	Student s1;//agar humko ye dubara print karana ho to Student s2 ya s1 ke alava kusch or likhana hoga.
	
	s1.dis();
	Student s2;
	s2.dis();
}
/*output:
:::HELLO:::
id=10
name=mustved
:::HELLO:::
id=10
name=mustved
*/
