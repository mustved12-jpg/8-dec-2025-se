PRO.2                                                                          <<16-02-26>>
#include<iostream>
using namespace std;
class Student
{
	public:
		int id;
		string name;
		
		Student()
		{
			cout<<":::HELLO:::";
			id=10;
			name="mustved";
		}
		void dis()
		{
			cout<<"\nid="<<id<<"\nname="<<name;
		}
};
main()
{
	Student s1;
	s1.dis();
}
/*output:
:::HELLO:::
id=10
name=mustved
*/
