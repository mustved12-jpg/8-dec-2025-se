#include<stdio.h>
void sub(int,int,int); //whit perameter
void main()
{
    
int a,b,c;  // agar me parmeter ke sath kar raha hu to mujhe ye to likhna hi hoga.

printf("enter a number1:");//agar me isko main mai likhunga to yeek bar number leg aor fix ho jaye ga
scanf("%d",&a);

printf("enter a number2:");
scanf("%d",&b);

      sub(a,b,c);
      printf("\n");
      sub(a,b,c);
      printf("\n");
      sub(a,b,c);
      
}
void sub(int num1,int num2,int)        // iski jgha agar me (int,int,int) likhunga to a,b,c sub mix go jaye ga 
                                       // or jvab galta aye ga 
{                                      //e.g  void sub(int,int,int)or variable diclaration niche karung like     
                                       //        int num1,num2,num3;    
    int num3=num1+num2;             //iske liye mujhe a ki jgha pe num1,or b ki jgah pe num2 rakhna hoga.
    printf("ans=%d",num3);
}
