#include<stdio.h>
void sub(int,int); //whit perameter
void main()
{
    
int a,b;  // agar me parmeter ke sath kar raha hu to mujhe ye to likhna hi hoga.


      sub(a,b);
      printf("\n");
      sub(a,b);
      printf("\n");
      sub(a,b);
      
}
void sub(int,int)        
{               
    int num1,num2;
printf("enter a number1:");// isme ye hur bar puche ga jese jab void main sub(a,b); mai jaye ga
                           // to pehle num1,num2 puche ga fir new line main jaye ga or ja dubara sub(a,b)
                           // mai jaye ga to fir puche ga num1,num2.
scanf("%d",&num1);

printf("enter a number2:");
scanf("%d",&num2);
    int num3=num1+num2; 
    printf("ans=%d",num3);
}
