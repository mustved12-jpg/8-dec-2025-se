pro.2                                                                                  <<19-1-26>>          

#include<stdio.h>
void main()
{
    int a[5]={10,30,1,2,32};
    for(int i=0;i<5;i++)
    {
        printf("\n Eliment(%d)=%d",i,a[i]);
    }
}
/*
 Eliment(0)=10
 Eliment(1)=30
 Eliment(2)=1
 Eliment(3)=2
 Eliment(4)=32
 */
