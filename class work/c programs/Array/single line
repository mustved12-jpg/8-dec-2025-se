pro.5                                                                           <<20-1-26>>
#include<stdio.h>
void main()
{
    int a[10]={12,23,34,45,456,3,3,454,244};
    int max=a[0];
    for(int i=1;i<10;i++)
    {
        if(max<a[i])
        {
            max=a[i];
        }
    }
    printf("max number:%d",max);
}
/*
output:
max number:456
*/
