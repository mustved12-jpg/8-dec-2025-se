pro.1                                                                               <<19-1-26>>

#include<stdio.h>
void main()
{
    int a[5]={12,23,43,545,232};
     printf("ans=%d",a[0]);
     printf("\nans=%d",a[3]);
}
/*output: ans=12
          ans=545
          */
