pro.4                                                                           <<20-1-26>>
#include<stdio.h>
void main()
{
    int sum=0;
    int a[10]={12,6,23,101,72,31,3};
    for(int i=0;i<10;i++)
    {
        printf("%d\n",a[i]);
        sum+=a[i];
        
    }
    printf("sum=%d\n",sum);
}
/*
output:
12
6
23
101
72
31
3
0
0
0
sum=248
*/
