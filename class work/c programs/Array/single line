pro.6                                                                           <<20-1-26>>
#include<stdio.h>
void main()
{
    int a[10]={12,23,34,45,456,3,3,454,244};
    int swipe=0;
    for(int i=0;i<10;i++)
    {
         printf("\npele=%d",a[i]);
    }
    for(int i=0;i<10;i++)
    {
       for(int j=i+1;j<10;j++)
       {
           if(a[i]<a[j])
           {
               swipe=a[i];
               a[i]=a[j];
               a[j]=swipe;
           }
       }
    }
    printf("\n---------------------------------\n");
  for(int i=0;i<10;i++)
  {
      printf("\nbad me =%d",a[i]);
  }
}
/*
output:

pele=12
pele=23
pele=34
pele=45
pele=456
pele=3
pele=3
pele=454
pele=244
pele=0
---------------------------------

bad me =456
bad me =454
bad me =244
bad me =45
bad me =34
bad me =23
bad me =12
bad me =3
bad me =3
bad me =0
*/