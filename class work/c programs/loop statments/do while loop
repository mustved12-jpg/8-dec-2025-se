pro.1                                                                                   <<10-1-26>>
#include<stdio.h>
void main()
{
    int i=1;  //initialization
    do
    {
        printf("%d\n",i);
        i++;
    }
    while(i<=5);
}
/*output:
1 
2 
3 
4 
5 
*/
