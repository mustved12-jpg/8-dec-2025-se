pattern.7                                                                                  <<8-1-26>>

#include<stdio.h>
void main()
{
    int k=1;
    for(int i=1;i<=4;i++)
    {
        for(int j=1;j<=i;j++)
        {
            printf("%d", k);
            k++;
        }
        printf("\n");
    }
}
/*
output
1
23
456
78910
*/
---------------------------------------------------------------------------------------------------------
pattern.8                                                                                <<8-1-26>>

#include<stdio.h>
void main()
{
    int ch=65;
    for(int i=1;i<=26;i++)
    {
        printf("%c",ch);
        ch++;
    }
}
/*
output:ABCDEFGHIJKLMNOPQRSTUVWXYZ
*/
---------------------------------------------------------------------------------------------------------
