pattern 1                                                                                  <8-1-26>

#include<stdio.h>
void main()
{
    for(int i=1;i<=5;i++)
    {
        printf("\n%d",i);
        for(int j=1;j<=3;j++)
        {
            printf("#");
        }
    }
}
/*
output=1###
       2###
       3###
       4###
       5###
*/
-------------------------------------------------------------------------------------------------------
pattern 2                                                                                  <8-1-26>

#include<stdio.h>
void main()
{
    for(int i=1;i<=5;i++)
    {
        for(int j=1;j<=5;j++)
        {
        printf("*");
        }
        printf("\n");
    }
}
/*
output
*****
*****
*****
*****
*****
*/
----------------------------------------------------------------------------------------------------------------
pattern 3                                                                                  <8-1-26>

#include<stdio.h>
void main()
{
    for(int i=1;i<=5;i++)
    {
        for(int j=1;j<=5;j++)
        if(i==3&&j==3)
        {
            printf("$");
        }
        else
        {
            printf("*");
        }
        printf("\n");
    }
}
/*
output
*****
*****
**$**
*****
*****
*/
--------------------------------------------------------------------------------------------------------
