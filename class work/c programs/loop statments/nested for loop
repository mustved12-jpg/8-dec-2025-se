pattern.7                                                                                  <<8-1-26>>

#include<stdio.h>
void main()
{
    int k=1;
    for(int i=1;i<=4;i++)
    {
        for(int j=1;j<=i;j++)
        {
            printf("%d", k);
            k++;
        }
        printf("\n");
    }
}
/*
output
1
23
456
78910
*/
---------------------------------------------------------------------------------------------------------
pattern.8                                                                                <<8-1-26>>

#include<stdio.h>
void main()
{
    int ch=65;
    for(int i=1;i<=26;i++)
    {
        printf("%c",ch);
        ch++;
    }
}
/*
output:ABCDEFGHIJKLMNOPQRSTUVWXYZ
*/
---------------------------------------------------------------------------------------------------------
pattern.9                                                                                 <<8-1-26>>

#include<stdio.h>
void main()
{
    int c=65;
    for(int i=1;i<=7;i++)
    {
        for(int j=1;j<=i;j++)
        {
            printf("%c",c);
            c++;
            if(c>'Z')
            {
                break;
            }
        }printf("\n");
    }
}
/*output:
A
BC
DEF
GHIJ
KLMNO
PQRSTU
VWXYZ
*/
-----------------------------------------------------------------------------------------------------------
pattern.10                                                                              <<10-1-26>>

#include<stdio.h>
void main()
{
    for (int i=1;i<=5;i++)
    {
        for(int j=5;j>=i;j--)
        {
            printf("*");
        }
        printf("\n");
    }
}
/*
output:
******
****
***
**
*
*/
----------------------------------------------------------------------------------------------------------
pattern.11                                                                              <<10-1-26>>

#include<stdio.h>
void main()
{
    for(int i=1;i<=4;i++)
    {
        
        for(int j=1;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    for(int k=5;k>=1;k--)
    {
        for(int p=1;p<=k;p++)
        {
            printf("*");
            
        }printf("\n");
    }
}
/*
output:
*
**
***
****
*****
****
***
**
*

*/
------------------------------------------------------------------------------------------------------------
