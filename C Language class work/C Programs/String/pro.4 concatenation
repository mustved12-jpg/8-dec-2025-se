#include<stdio.h>
#include<string.h>
void main()
{  int i=0;
   char name1[20],name2[20];
   printf("enter a name 1:");
   gets(name1);
   printf("enter a name 2:");
   gets(name2);
   
  // strcat(name1,name2);
   //printf("\n%s",name1);
   for(i;i<strlen(name1);i++)
   {
       
   }
   for(int j=0;j<strlen(name2);j++)
   {
       name1[i]=name2[j];
       i++;
   }
   printf("\n%s",name1);
   
}
