                               inbuilt
PRO.1                                                           <<30-01-26>>                                                                               <<30-01-26>>
#include<stdio.h>
#include<string.h>
void main()
{
    char name1[20];
    char name2[20];
    printf("enter a firs name:-");
    gets(name1);
    printf("enter a secound name:-");
    gets(name2);
    printf("%s",strcat(name1,name2));
    printf("\n\n%s",name1);
    
}
/*
out put:
enter a firs name:-mohammed
enter a secound name:-mustved
mohammedmustved

mohammedmustved
*/
