                                     without inbuilt
PRO.1                                                                         <<04-02-26>>
#include<stdio.h>
#include<string.h>
void main()
{
    char name1[20];
    char name2[20];
    int f=0;
    printf("enter a name 1:");
    gets(name1);
    printf("enter a name 2:");
    gets(name2);
    for(int i=0;name1[i]!='\0'||name2[i]!='\0';i++)
    {
     if(name1[i]!=name2[i])
     {
        f=1;
        break;
     }
     
    }
    if(f==0)
    {
        printf("sem");
    }
    else
    {
        printf("not sem");
    }
}