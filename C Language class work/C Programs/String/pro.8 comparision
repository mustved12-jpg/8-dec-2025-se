PRO.1                                                                   <<04-02-26>>
#include<stdio.h>
#include<string.h>
int main()
{
    int i;
	char name1[90];
	char name2[90];
	printf("enter a name 1:");
	gets(name1);
	printf("enter a name 2:");
	gets(name2);
	if(strcmp(name1,name2)=='\0')
	{
	    printf("sem name.^-^.");
	}
	else
	{
	    printf("name is not sem.*_*");
	}
     return 0;
}
