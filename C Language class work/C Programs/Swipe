                                   swipe:
#include<stdio.h>
void main()
{
    int a,b,c;
    a=10;
    b=20;
   c=a;
   a=b;
   b=c;
    printf("ans=%d",a);
    printf("\nans=%d",b);
}

         /* output: ans=20
                    ans=10 */

// agar hamko bola jaye ki 3 variable nhi lena hai tab ham ese swipe karenge


  a=a+b  //(10+20) abhi a=30 ho gya hai 
  b=a-b  //(30-20) abhi b=10 ho gya hai
  a=a-b  //(30-10) abhi a=20 ho gya ese hoga.