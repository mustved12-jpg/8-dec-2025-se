PRO.1                                                                          <<11-02-26>>
#include<stdio.h>
main()
{
	FILE *fp;
	fp=fopen("my file.txt","w");
	fprintf(fp,"hello this is my first file programe");
	fclose(fp);
}
/*in file:
hello this is my first file programe
*/
