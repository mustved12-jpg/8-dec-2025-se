#include<stdio.h>
main()
{
	FILE *fp;
	fp=fopen("my file.1.txt","w");
	fprintf(fp,"mustved C Language developer\n");
	fprintf(fp,"score:76\n");
    printf("success fully file created");
	fclose(fp);
	
}
/* in file:
mustved C Language developer
score:76

in output screen:
success fully file created
*/
