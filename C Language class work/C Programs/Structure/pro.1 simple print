PRO.1                                                                           <<06-02-26>>
#include<stdio.h>
#include<string.h>
struct student
{
	char name[20];
	int id;
	float marks;
};
int main()
{
	struct student s;
	strcpy(s.name,"mustved");// ye direct nhi aye ga (s.name="mustved";)ye rong hai
	s.id=3;               
	s.marks=76.34;
	printf("student name is:%s",s.name);
	printf("\nroll number:%d",s.id);
	printf("\nmarks:%.2f",s.marks);
	
}
/*
output:
student name is:mustved
roll number:3
marks:76.34
*/
