pro.4                                                                                   <<24-01-26>>
#include<stdio.h>
void main()
{
    int max,a[2][4]={{10,20,30,5},
                 {40,50,60}};
                 max=a[0][0];
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<=3;j++)
        {
            printf("%d ",a[i][j]);
            
        }
        printf("\n");
        for (int k=1;k<8;k++)
        {
            if(max<a[i][k])
            {
                max=a[i][k];
            }
        }
    }printf("\nmax number:%d",max);
}
/*
output:
10 20 30 5 
40 50 60 0 

max number:60
*/
---------------------------------------------------------------------------------------------------------
pro.5                                                                                   <<24-01-26>>
#include<stdio.h>
void main()
{
    int a[2][4]={{10,20,30,40},{50,55,63}};
    int max=0;
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<4;j++)
        {
            printf("%d ",a[i][j]);
            if(a[i][j]<a[i][++j])
            {
                max=a[i][j];
            }
            --j;
        }
        printf("\n");
        
    }
    printf("maximum number=%d",max);
}
/*
output:
10 20 30 40 
50 55 63 0 
maximum number=63
*/
---------------------------------------------------------------------------------------------------------
