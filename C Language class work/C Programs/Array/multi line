pro.1                                                                                  <<23-01-26>>
#include<stdio.h>
void main(){
    int a[2][3]={{10,20,30},    //a[2] ye row hai a[3] ye collame hai
                 {40,50,60}};
                 printf("ans1=%d",a[0][1]);
                 printf("\nans2=%d",a[1][2]);
}
/*
output:
ans1=20
ans2=60
*/
