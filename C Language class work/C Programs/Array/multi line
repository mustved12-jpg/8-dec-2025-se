pro.2                                                                                   <<24-01-26>>
#include<stdio.h>
void main(){
    int a[2][3]={{10,20,30},    //a[2] ye row hai a[3] ye collame hai
                 {40,50,60}};

        for(int i=0;i<2;i++)
        {
            for(int j=0;j<3;j++)
            {
                printf(" row(%d)collame(%d)=%d",i,j,a[i][j]);
            }
            printf("\n");
        }
}
/*
output:
row(0)collame(0)=10 row(0)collame(1)=20 row(0)collame(2)=30
row(1)collame(0)=40 row(1)collame(1)=50 row(1)collame(2)=60
*/
------------------------------------------------------------------------------------------------------------
pro.3                                                                                   <<24-01-26>>
#include<stdio.h>
void main()
{
    int i,j,a[2][3];
    for(int i=0;i<2;i++)
    {
        for(j=0;j<3;j++)
        {
            
        printf("enter a (%d,%d) number:",i,j);
        scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<2;i++)
    {
    for(j=0;j<3;j++)
    {
        printf("%d",a[i][j]);
        
    }
    printf("\n");
    }
}
/*
output:
enter a (0,0) number:10
enter a (0,1) number:20
enter a (0,2) number:30
enter a (1,0) number:40
enter a (1,1) number:50
enter a (1,2) number:60
102030
405060
*/
