#include <stdio.h>

int main() {
    int n, i;
    
    printf("Enter any number: ");
    scanf("%d", &n);
    
    if (n <= 1) {
        printf("%d is NOT a prime number\n", n);
        return 0;
    }
    
    for (i = 2; i < n; i++) 
    {
        if (n % i == 0) 
        {
            printf("%d is NOT a prime number\n", n);
            return 0;   // number divide ho gaya â†’ prime nahi
        }
    }
    
    printf("%d is a PRIME number\n", n);
    return 0;
}